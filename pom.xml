  <groupId>org.example</groupId>
  <artifactId>daCoffee</artifactId>
  <packaging>war</packaging>
  <version>1.0-SNAPSHOT</version>
  <name>Coffee Maven Webapp</name>
  <url>http://maven.apache.org</url>

  <properties>
